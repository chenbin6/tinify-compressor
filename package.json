{
    "name": "@fe/image-compressor-tool",
    "version": "1.0.5",
    "description": "A powerful image compression tool with caching support",
    "main": "lib/compressor.js",
    "type": "module",
    "bin": {
        "compress": "scripts/index.js"
    },
    "scripts": {
        "test": "node scripts/index.js",
        "compress": "scripts/index.js",
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "publish:private": "npm publish",
        "publish:private:beta": "npm publish --tag beta"
    },
    "keywords": [
        "image",
        "compression",
        "tinify",
        "optimizer"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "crypto": "^1.0.1",
        "tinify": "^1.8.1"
    },
    "files": [
        "lib/",
        "scripts/"
    ],
    "devDependencies": {
        "@rollup/plugin-terser": "^0.4.4",
        "rollup": "^4.50.0"
    }
}
